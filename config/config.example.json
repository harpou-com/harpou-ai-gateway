{
    "llm_backends": [
        {
            "name": "ollama_local",
            "type": "ollama",
            "base_url": "http://localhost:11434",
            "default_model": "llama3",
            "api_key": "NA", 
            "timeout": 600,
            "llm_auto_load": true
        },
        {
            "name": "openai_cloud",
            "type": "openai",
            "base_url": "https://api.openai.com/v1",
            "default_model": "gpt-4-turbo",
            "api_key": "YOUR_OPENAI_API_KEY_HERE",
            "timeout": 120,
            "llm_auto_load": false
        },
        {
            "name": "lm_studio_local",
            "type": "openai",
            "base_url": "http://localhost:1234/v1",
            "default_model": "lmstudio-community/Meta-Llama-3-8B-Instruct-GGUF",
            "api_key": "NA",
            "llm_auto_load": true
        }
    ],
    "primary_backend_name": "ollama_local",
    "routing_backend_name": "lm_studio_local",
    "high_availability_strategy": "failover",
    "SYSTEM_ADMIN_EMAIL": "admin@example.com",
    "LLM_BACKEND_TIMEOUT": 300,
    "SEARXNG_BASE_URL": "http://localhost:8080",
    "FLASK_SECRET_KEY": "a_very_secret_key_for_local_dev_only",
    "CELERY_BROKER_URL": "redis://redis:6379/0",
    "CELERY_RESULT_BACKEND": "redis://redis:6379/0",
    "RATELIMIT_STORAGE_URI": "redis://redis:6379/0",
    "CACHE_REDIS_URL": "redis://redis:6379/0",
    "LOG_LEVEL": "info",
    "LOG_ROTATION_DAYS": 7,
    "llm_cache_update_interval_minutes": 5,
    "reasoning_loop_budget": 5,
    "reasoning_time_budget_seconds": 45,
    "background_loop_budget": 10,
    "force_background_on_budget_exceeded": true,
    "api_keys": [
        {
            "key": "sk-my-first-key-for-user1",
            "username": "user_1",
